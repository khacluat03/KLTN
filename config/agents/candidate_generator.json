{
    "model_type": "openai",
    "model_name": "gpt-4o-mini",
    "temperature": 0.4,
    "max_tokens": 1024,
    "json_mode": true,
    "default_candidate_size": 50,
    "max_turns": 10,
    "tool_config": {
        "info_retriever": {
            "type": "info",
            "config_path": "config/tools/info_database/{dataset}.json"
        },
        "interaction_retriever": {
            "type": "interaction",
            "config_path": "config/tools/interaction/{dataset}.json"
        },
        "cf_tool": {
            "type": "collaborative_filtering",
            "config_path": "config/tools/collaborative_filtering.json"
        }
    }
}